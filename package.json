{
  "name": "jest-tests-api",
  "version": "v1.0.1",
  "main": "index.js",
  "author": "Matheus Ribeiro <MaverickHenrique1010@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 16 <= 16.14.0",
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test && yarn build"
    }
  },
  "lint-staged": {
    "**/*.+(js|json|css|html|md)": [
      "prettier",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "scripts": {
    "start": "node build/index.js",
    "start:dev": "NODE_ENV=development nodemon --delay 500ms --inspect --exec babel-node index.js",
    "lint": "eslint --ext .js ./src",
    "lint:fix": "yarn lint --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@bugsnag/js": "^7.16.1",
    "@bugsnag/plugin-express": "^7.16.1",
    "@bugsnag/source-maps": "^2.3.1",
    "@testing-library/jest-dom": "testing-library/jest-dom",
    "aws-sdk": "^2.1071.0",
    "bcryptjs": "^2.4.3",
    "bwip-js": "^3.0.4",
    "chalk": "4",
    "cors": "^2.8.5",
    "currency.js": "^2.0.4",
    "dotenv": "^16.0.0",
    "exceljs": "^4.3.0",
    "express": "^4.17.2",
    "handlebars": "^4.7.7",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^13.2.0",
    "redis": "^3.1.2",
    "sanitize-html": "^2.7.0",
    "sequelize": "^6.16.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/register": "^7.17.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "copy": "^0.3.2",
    "eslint": "^8.8.0",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "lint-staged": "^13.1.2",
    "nodemon": "^2.0.20",
    "semver": "^7.3.5"
  },
  "resolutions": {
    "graceful-fs": "4.x.x"
  }
}
